<h1>Add  a new user</h1>
<div class="form">
<%= error_messages_for :member %>
  <% form_for :member, :url => members_path do |f| -%>
    <table>
      <%= render :partial => "form", :locals => {:f => f} %>
      <tr><th>&nbsp;</th><td><%= submit_tag 'Sign up' %> or <%= link_to "Cancel", members_path %></td></tr>
    </table>
  <% end -%>
</div>